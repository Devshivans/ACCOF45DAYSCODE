#include<stdio.h>
#include<stdlib.h>
void main(){
    int *p,i,n;
    printf("enter the no. of variables\n");
    scanf("%d",&n);
    p =(int*)malloc(n* sizeof(int));
    printf("enter %d values\n",n);
    for(i=0;i<n;i++){
    scanf("%d",(p+i));
    }
    printf("the %d values are",n);
    for(i=0;i<n;i++)
    printf("%d",*(p+i));
}
