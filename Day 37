#include<stdio.h>  
int main()    
{    
int n,r,sum=0,temp;    
printf("\n Enter the number : ");    
scanf("%d",&n);    
temp=n;    
while(n>0)    
{    
r=n%10;    
sum=(sum*10)+r;    
n=n/10;    
}    
if(temp==sum)    
printf("The No. %d is a palindrome number ",n);    
else    
printf("The No. %d is NOT a palindrome number ",n);  
return 0;  
}
