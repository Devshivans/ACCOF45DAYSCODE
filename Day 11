#include<stdio.h>
void main(){
    int n;
    printf("enter n");
    scanf("%d",&n);
    for(int i=n;i>=1;i--){
        for(int j=1;j<=n;j++){
            if((i+j)<=n){
                printf(" ");
            }  else {
                printf("*");
            }

        }
        printf("\n");
    }
}
